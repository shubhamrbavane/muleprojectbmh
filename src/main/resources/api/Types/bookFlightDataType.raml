#%RAML 1.0 DataType
type: object
properties:
  
  passengerDetails:
    type: object
    properties:
      firstName: string
      lastName: string
      age: integer
      gender: string
      contactDetails:
        type: object
        properties:
          mobileNumber: integer
          emailID: string
  flightDetails:   
    type: object
    properties:     
      flightCode: string
      airline: string
      classType: string